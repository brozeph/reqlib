{
  "name": "reqlib",
  "version": "1.0.12",
  "description": "A library to simplify REST API requests and to provide utilities for handling common REST API consumer scenarios ",
  "main": "dist",
  "scripts": {
    "build": "gulp",
    "lint": "gulp lint",
    "posttest": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "gulp build",
    "pretest": "gulp clean && gulp lint",
    "test": "nyc mocha --recursive",
    "test:unit": "nyc mocha --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/brozeph/reqlib.git"
  },
  "keywords": [
    "https",
    "rest",
    "api",
    "client",
    "request",
    "library"
  ],
  "author": "Joshua Thomas (github.com/brozeph)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brozeph/reqlib/issues"
  },
  "homepage": "https://github.com/brozeph/reqlib#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.18.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-eslint": "^6.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "mocha": "^8.2.1",
    "nock": "^13.0.5",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.12.5"
  }
}
